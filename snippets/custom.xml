<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <!-- remote -->
  <remote name="github"
          fetch="https://github.com/" />

  <remote name="lineage"
          fetch="https://github.com/LineageOS"
          revision="lineage-17.0" />

  <!-- remove repos -->
  <include name="snippets/remove.xml" />

  <!-- CodeAurora additions -->
  <project path="vendor/qcom/opensource/power" name="AOSP-10/vendor_qcom_opensource_power" revision="10" remote="github" />
  
  <!-- External repos -->
  <project path="external/chromium-webview" name="AOSP-10/external_chromium-webview" revision="10" remote="github" />

  <!-- General repos -->
  <project path="build/blueprint" name="AOSP-10-CAF/build_blueprint" revision="10" remote="github" />
  <project path="build/make" name="AOSP-10-CAF/build" revision="10" remote="github">
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/soong" name="AOSP-10-CAF/build_soong" revision="10" remote="github" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>
  <project path="vendor/aosp" name="AOSP-10-CAF/vendor_aosp" revision="10" remote="github" />

</manifest>
